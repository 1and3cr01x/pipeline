#!/bin/bash

echo "PIO_MODEL_NAMESPACE=$PIO_MODEL_NAMESPACE"
echo "PIO_MODEL_NAME=$PIO_MODEL_NAME"
echo "PIO_MODEL_VERSION=$PIO_MODEL_VERSION"
echo "PIO_BUNDLE_SERVER_PORT=$PIO_BUNDLE_SERVER_PORT"
echo "STORE_HOME=$STORE_HOME"

source activate "${PIO_MODEL_NAMESPACE}_${PIO_MODEL_NAME}_${PIO_MODEL_VERSION}"

mkdir -p $STORE_HOME/$PIO_MODEL_NAMESPACE/$PIO_MODEL_NAME/$PIO_MODEL_VERSION/

#cd $STORE_HOME/$PIO_MODEL_NAMESPACE/$PIO_MODEL_NAME/$PIO_MODEL_VERSION/

#PYTHONPATH=$STORE_HOME/$PIO_MODEL_NAMESPACE/$PIO_MODEL_NAME/$PIO_MODEL_VERSION/:$PYTHONPATH 
src/main/python/bundle/start_bundle_server.py $PIO_BUNDLE_SERVER_PORT 
